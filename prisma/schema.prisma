// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model RawPassing {
  id            Int      @id
  pid           Int
  timingPoint   String
  result        Int
  time          Float
  invalid       Boolean
  bib           Int

  // Passing fields
  transponder    String
  hits           Int
  rssi           Int
  battery        Int
  temperature    Int
  wuc            Int
  loopId         Int
  channel        Int
  internalData   String
  statusFlags    Int
  deviceId       String
  deviceName     String
  orderId        Int
  port           Int
  isMarker       Boolean
  fileNo         Int
  passingNo      Int
  customer       Int
  received       DateTime
  utcTime        DateTime

  // Position fields
  latitude       Float
  longitude      Float
  altitude       Float
  flag           String
}
